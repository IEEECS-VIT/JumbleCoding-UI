<Window x:Class="JumbleCoding.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Login" ResizeMode="CanMinimize" Height="300" Width="300">
    <StackPanel>
        <TextBlock Text="Welcome!"></TextBlock>
        <ListView></ListView>
        <TextBlock Text="Registration Number"></TextBlock>
        <TextBox></TextBox>
        <Button Content="login" Click="LoginButton_Click"></Button>
    </StackPanel>
</Window>